## Metrics Over Time


<div>
  <canvas id="qcChart"></canvas>

  <script src="https://cdn.jsdelivr.net/npm/chart.js"></script>
  <script>
    const allCharts = {{ charts_json }};
    const chartIds = Object.keys(allCharts || {});
    const defaultChart = chartIds.length ? allCharts[chartIds[0]] : { labels: [], datasets: [] };
    const ctx = document.getElementById('qcChart');

    new Chart(ctx, {
      type: 'line',
      data: defaultChart,
      options: {
        responsive: true,
        maintainAspectRatio: false,
        interaction: {
          mode: 'index',
          intersect: false
        },
        scales: {
          y: {
            beginAtZero: false
          }
        }
      }
    });
  </script>
</div>


